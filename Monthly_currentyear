' Dynamic date range script for SAP GUI
Dim currentDate, startMonth, startYear, i
Dim monthStart, monthEnd, dateFieldLow, dateFieldHigh

' Get current date
currentDate = Date()
startMonth = Month(currentDate)
startYear = Year(currentDate)

' Loop through 12 months starting from current month
For i = 1 To 12
    ' Calculate the month and year for current iteration
    Dim targetMonth, targetYear
    targetMonth = ((startMonth - 1 + (i - 1)) Mod 12) + 1
    targetYear = startYear + Int((startMonth - 1 + (i - 1)) / 12)
    
    ' Get first day of the month
    monthStart = DateSerial(targetYear, targetMonth, 1)
    
    ' Get last day of the month
    monthEnd = DateSerial(targetYear, targetMonth + 1, 1) - 1
    
    ' Format dates as DD.MM.YYYY
    Dim formattedStart, formattedEnd
    formattedStart = Right("0" & Day(monthStart), 2) & "." & Right("0" & Month(monthStart), 2) & "." & Year(monthStart)
    formattedEnd = Right("0" & Day(monthEnd), 2) & "." & Right("0" & Month(monthEnd), 2) & "." & Year(monthEnd)
    
    ' Set the date fields
    dateFieldLow = "wnd[0]/usr/ctxtP_DATE" & i & "-LOW"
    dateFieldHigh = "wnd[0]/usr/ctxtP_DATE" & i & "-HIGH"
    
    session.findById(dateFieldLow).text = formattedStart
    session.findById(dateFieldHigh).text = formattedEnd
    
    ' Optional: Add some logging to see what dates are being set
    ' WScript.Echo "Month " & i & ": " & formattedStart & " to " & formattedEnd
Next

