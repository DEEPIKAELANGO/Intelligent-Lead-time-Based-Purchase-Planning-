Set WshShell = WScript.CreateObject("WScript.Shell")
Set WshShellpy = WScript.CreateObject("WScript.Shell")
Dim exeName 
Dim statusCode
exeName = """C:\Program Files (x86)\SAP\SAPGUI770\SAPgui\saplogon.exe"""
WshShell.Run(exeName)
WScript.Sleep 5000

If Not IsObject(App) Then
   Set SapGuiAuto = GetObject("SAPGUI")
   Set App = SapGuiAuto.GetScriptingEngine
End If
If Not IsObject(Connection) Then
    Set Connection = App.OpenConnection("- P20 Production ERP Logistics and Finance")
End If
If Not IsObject(session) Then
   Set session = Connection.Children(0)
    
End If
'If IsObject(WScript) Then
'   WScript.ConnectObject session, "on"
'   WScript.ConnectObject Application, "on"
'End If

Set objFSO = CreateObject("Scripting.FileSystemObject")
Set objFile = objFSO.OpenTextFile("Credentials.txt", 1)
count = 0
Dim fileLines()

Do
  count = count + 1
  Redim PRESERVE fileLines(count)
  fileLines(count) = trim(Mid(objFile.ReadLine,1,500))
Loop Until objFile.AtEndOfStream

'wscript.echo fileLines(3)
sappass = Trim(TextBox1)
session.findById("wnd[0]").maximize
session.findById("wnd[0]/usr/txtRSYST-BNAME").Text = fileLines(1)
session.findById("wnd[0]/usr/pwdRSYST-BCODE").Text = fileLines(2)
session.findById("wnd[0]/usr/pwdRSYST-BCODE").SetFocus
